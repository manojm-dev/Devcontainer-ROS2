{
	"name": "ROS Workspace",
	"dockerComposeFile": ["compose.yml"],
	"service":"ros",

	"shutdownAction": "stopCompose",
	"workspaceFolder": "/home/develop/workspace/src",

	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools-extension-pack",
				"eamodio.gitlens",
				"ms-iot.vscode-ros",
				"ms-azuretools.vscode-docker"
			]
		}
	}
}