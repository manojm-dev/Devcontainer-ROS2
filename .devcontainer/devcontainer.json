{
	"name": "ROS Workspace",
	"dockerComposeFile": ["compose.yml"],
	"service":"ros",

	"shutdownAction": "stopCompose",
	"workspaceFolder": "/home/develop/workspace/",

	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-vscode.cpptools-extension-pack",
				"eamodio.gitlens",
				"ms-iot.vscode-ros"
			]
		}
	}
}